{
  "name": "Codestrap",
  "version": "0.0.1",
  "description": "Extension to bootstrap a new project",
  "permissions": [
    "storage",
    "https://*.github.com/",
    "identity"
  ],
  "background": {
    "scripts": [
      "background.bundle.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Code Strap",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/16px.png",
      "32": "images/32px.png",
      "64": "images/64px.png",
      "128": "images/128px.png"
    }
  },
  "icons": {
    "16": "images/16px.png",
    "32": "images/32px.png",
    "64": "images/64px.png",
    "128": "images/128px.png"
  },
  "manifest_version": 2
}